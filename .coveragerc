[run]
branch = True
source =
    src
omit =
    */__init__.py
    src/core/fixture_record.py
    deprecated/*
    tests/*
    */.venv/*
    */site-packages/*

[report]
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:
    raise NotImplementedError
    pass
